[remote "fastai"]
    url = git@github.com:fastai/fastbook.git
    fetch = +refs/heads/*:refs/remotes/fastai/*
[remote "origin"]
    url = git@github.com:huangyingw/fastai_fastbook.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = fastai
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = .gitmodules
    ignore = 01_intro.py
    ignore = 02_production.py
    ignore = 03_ethics.py
    ignore = 04_mnist_basics.py
    ignore = 05_pet_breeds.py
    ignore = 06_multicat.py
    ignore = 07_sizing_and_tta.py
    ignore = 08_collab.py
    ignore = 09_tabular.py
    ignore = 10_nlp.py
    ignore = 11_midlevel_data.py
    ignore = 12_nlp_dive.py
    ignore = 13_convolutions.py
    ignore = 14_resnet.py
    ignore = 15_arch_details.py
    ignore = 16_accel_sgd.py
    ignore = 17_foundations.py
    ignore = 18_CAM.py
    ignore = 19_learner.py
    ignore = 20_conclusion.py
    ignore = app_blog.py
    ignore = app_jupyter.py
    ignore = clean/*.py
    ignore = exercise/*
    ignore = fastai
    ignore = pytorch
    ignore = vision
[deploy]
	host = localhost
	path = ~/myproject/git/AI/fastai/fastbook
